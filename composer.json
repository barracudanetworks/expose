{
    "name":"barracudanetworks/expose",
    "type":"library",
    "description":"An intrusion detection library based on PHPIDS",
    "keywords":["ids","intrusion","detection"],
    "homepage":"https://github.com/barracudanetworks/expose.git",
    "license":"MIT",
    "require":{
        "php":">=8.1",
        "psr/log": "~3.0",
        "psr/simple-cache": "^3.0",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev" : {
        "phpunit/phpunit": "9.*",
        "symfony/cache": "^6.4 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Expose\\": "src/Expose/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Expose\\": "tests/"
        }
    }
}
